      \strng{namehash}{b8961bbedd94ef4bfba198168e9d9220}
      \strng{fullhash}{b8961bbedd94ef4bfba198168e9d9220}
      \strng{fullhashraw}{b8961bbedd94ef4bfba198168e9d9220}
      \strng{bibnamehash}{b8961bbedd94ef4bfba198168e9d9220}
      \strng{authorbibnamehash}{b8961bbedd94ef4bfba198168e9d9220}
